<!DOCTYPE html encode="utf-8">
	<html lang="pt-br">
		<head>
			<meta charset="utf-8"/>
			<title>Notícias</title>
		</head>
	<body>

		<h1>Notícias</h1>

		<table>
			<trh>
				<td>Id</td>
				<td>Titulo</td>
				<td>Notícia</td>
			</tr>

			<% for(var i=0; i < noticias.length; i++) { %>
				<tr>
					<td><%= noticias[i].id %></td>
					<td><%= noticias[i].titulo %></td>
					<td><%= noticias[i].noticia %></td>
				</tr>
			<% } %>
		</table>

	</body>
</html>